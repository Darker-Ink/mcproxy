  "license": "MIT",
  "devDependencies": {
    "jest": "^26.1.0",
    "require-self": "^0.2.3",
    "standard": "^15.0.0"
  },
  "dependencies": {
    "minecraft-data": "^2.69.1",
    "minecraft-protocol": "^1.17.0",
    "mineflayer": "^2.32.0",
    "typescript": "^4.0.3"
  }
}
